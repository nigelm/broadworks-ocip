      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:token">
      <xs:minLength value="1"/>
      <xs:maxLength value="80"/>
    </xs:restriction>
  </xs:simpleType>    
    
  <xs:complexType name="VoiceMessagingAlternateNoAnswerGreetingRead16">
    <xs:annotation>
      <xs:documentation>
