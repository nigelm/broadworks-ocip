    <xs:annotation>
      <xs:documentation>
        Response to the EnterpriseCommonPhoneListGetListRequest.
        The response contains the enterprise's common phone list.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIDataResponse">
        <xs:sequence>
