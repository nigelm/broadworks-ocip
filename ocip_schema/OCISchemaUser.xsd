  <xs:complexType name="UserAccessDeviceFileGetListRequest">
    <xs:annotation>
      <xs:documentation>
        Request to get the list of Device Management user modifiable files.
        The response is either UserAccessDeviceFileGetListResponse or ErrorResponse.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIRequest">
        <xs:sequence>
          <xs:element name="userId" type="UserId"/>
          <xs:element name="accessDevice" type="AccessDevice"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>    
    
  <xs:complexType name="UserAccessDeviceFileGetListResponse">
    <xs:annotation>
      <xs:documentation>
        Response to UserAccessDeviceFileGetListRequest.
        Contains a table of user modifiable Device Management files.
        The column headings are: "File Format", "Template URL".
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIDataResponse">
        <xs:sequence>
          <xs:element name="deviceFilesTable" type="core:OCITable"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>    
    
  <xs:complexType name="UserAccessDeviceFileGetRequest">
    <xs:annotation>
      <xs:documentation>
        Request to get a user modifiable file.
        The response is either UserAccessDeviceFileGetResponse or ErrorResponse.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIRequest">
        <xs:sequence>
          <xs:element name="userId" type="UserId"/>
          <xs:element name="accessDevice" type="AccessDevice"/>
          <xs:element name="fileFormat" type="DeviceManagementFileFormat"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>    
    
  <xs:complexType name="UserAccessDeviceFileGetResponse">
    <xs:annotation>
      <xs:documentation>
        Response to UserAccessDeviceFileGetRequest.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="core:OCIDataResponse">
        <xs:sequence>
          <xs:element name="templateUrl" type="URL" minOccurs="0"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>    
    
  <xs:complexType name="UserAccessDeviceFileModifyRequest">
